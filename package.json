{
  "name": "leakstemplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cloudflare": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build && npm run post-build-optimize",
    "build:staging": "cross-env NODE_ENV=staging next build",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "pre-build": "npm run lint && npm run type-check",
    "post-build-optimize": "tsx scripts/post-build-optimize.ts",
    "fetch-images": "node scripts/fetch-image-list.mjs",
    "generate-content": "tsx scripts/generate-content.ts generate",
    "clear-content-cache": "tsx scripts/generate-content.ts clear-cache",
    "content-stats": "tsx scripts/generate-content.ts stats",
    "generate-sitemap": "node scripts/generate-sitemap.mjs",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:cache": "rm -rf .next/cache"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "dotenv": "^17.0.1",
    "framer-motion": "^12.23.3",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
